{
  "expo": {
    "name": "Nonvi Voyage Plus",
    "slug": "nonvi_voyage_plus",
    "scheme": "nonvi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image_app.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#FAF8F5",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/image_app.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a2a3a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bellox.nonvi",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.bellox.nonvi",
      "adaptiveIcon": {
        "foregroundImage": "./assets/image_app.png",
        "backgroundColor": "#1a2a3a"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "navigationBar": {
        "visible": "transparent"
      },
      "statusBar": {
        "translucent": true
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      "expo-font",
      "expo-asset",
      "expo-dev-client",
      [
        "expo-camera",
        {
          "cameraPermission": "L'application a besoin d'accéder à votre caméra pour scanner les tickets."
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "a68455bb-25ec-44d4-b75e-517b170f0636"
      }
    }
  }
}
